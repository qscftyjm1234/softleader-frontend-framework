{
  "name": "nuxt-app",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=4096 eslint . --fix",
    "build": "nuxt build",
    "build:staging": "cross-env NODE_ENV=staging nuxt build",
    "dev": "nuxt dev",
    "clean": "npx nuxi clean && rimraf node_modules",
    "reinstall": "npm run clean && npm install",
    "generate": "nuxt generate",
    "generate:staging": "cross-env NODE_ENV=staging nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "commit": "cz",
    "clone": "clone-project.bat",
    "open": "explorer ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "configs/git/cz-config.cjs"
    }
  },
  "dependencies": {
    "@nuxt/fonts": "0.11.0",
    "@nuxt/icon": "^1.11.0",
    "@nuxtjs/device": "3.2.4",
    "@nuxtjs/i18n": "10.0.3",
    "@pinia/nuxt": "^0.10.1",
    "@vueuse/core": "^10.11.1",
    "ant-design-vue": "^4.2.6",
    "dayjs": "1.11.13",
    "lodash.clonedeep": "4.5.0",
    "nuxt": "3.15.4",
    "pinia": "3.0.1"
  },
  "devDependencies": {
    "@ant-design-vue/nuxt": "^1.0.0",
    "cross-env": "7.0.3",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@nuxt/eslint": "1.2.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@types/lodash.clonedeep": "4.5.9",
    "@types/node": "25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "commitizen": "4.3.1",
    "cz-customizable": "7.5.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "50.8.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "^3.5.3",
    "sass": "^1.85.0",
    "typescript": "5.7.3",
    "vue-tsc": "2.2.0"
  }
}
