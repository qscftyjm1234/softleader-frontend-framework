{
  "name": "nuxt-app",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=4096 eslint . --fix",
    "build": "nuxt build",
    "build:staging": "cross-env NODE_ENV=staging nuxt build",
    "dev": "nuxt dev",
    "clean": "npx nuxi clean && rimraf node_modules",
    "reinstall": "npm run clean && npm install",
    "generate": "nuxt generate",
    "generate:staging": "cross-env NODE_ENV=staging nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "commit": "cz",
    "clone": "clone-project.bat",
    "open": "explorer ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "configs/git/cz-config.cjs"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/device": "3.2.4",
    "@nuxtjs/i18n": "10.0.3",
    "@pinia/nuxt": "0.11.2",
    "@vueuse/nuxt": "13.6.0",
    "ant-design-vue": "^4.2.6",
    "dayjs": "1.11.13",
    "lodash.clonedeep": "4.5.0",
    "nuxt": "3.15.2",
    "pinia": "3.0.3"
  },
  "devDependencies": {
    "@ant-design-vue/nuxt": "^1.0.0",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@nuxt/eslint": "1.12.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/lodash.clonedeep": "4.5.9",
    "@types/node": "25.0.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "commitizen": "4.3.1",
    "cz-customizable": "7.5.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "standard-version": "9.5.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3",
    "vue-tsc": "3.2.1"
  }
}
