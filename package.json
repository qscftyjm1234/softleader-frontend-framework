{
  "name": "nuxt-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint . --fix",
    "build": "nuxt build",
    "dev": "node scripts/dev.js",
    "dev:nuxt": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "gen:module": "node scripts/generate-module.js",
    "gen:watch": "node scripts/generate-module.js watch",
    "gen:all": "node scripts/generate-module.js all",
    "commit": "cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix --no-warn-ignored",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/i18n": "^10.0.3",
    "@pinia/nuxt": "^0.11.2",
    "@vueuse/nuxt": "^13.6.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "lodash.clonedeep": "^4.5.0",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint": "^1.12.1",
    "@playwright/test": "^1.55.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^25.0.3",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.5.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.1"
  }
}
