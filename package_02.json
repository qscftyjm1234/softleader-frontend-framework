{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "nuxt build",
    "dev": "npm run clean-static && nuxt dev",
    "dev-host": "npm run clean-static && nuxt dev --host",
    "generate": "nuxt generate",
    "generate:sit": "nuxt generate --dotenv .env.sit",
    "generate:sit2": "nuxt generate --dotenv .env.sit2",
    "generate:sit3": "nuxt generate --dotenv .env.sit3",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "prepare": "husky",
    "clean-static": "powershell -Command \"if (Test-Path .output) { Remove-Item -Recurse -Force .output }\""
  },
  "dependencies": {
    "@nuxt/eslint": "1.2.0",
    "@nuxt/fonts": "^0.11.0",
    "@nuxt/icon": "^1.11.0",
    "@vueuse/core": "^10.11.1",
    "@zxing/library": "^0.19.1",
    "ant-design-vue": "^4.2.1",
    "axios": "^1.7.9",
    "decimal.js": "^10.4.3",
    "fs-extra": "^11.3.0",
    "msw": "^2.7.0",
    "nuxt": "^3.15.4",
    "pdfjs-dist": "^5.1.91",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@pinia/nuxt": "^0.10.1",
    "@tailwindcss/postcss": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "24.0.0",
    "husky": "^9.0.11",
    "pinia": "^3.0.1",
    "postcss": "^8.5.2",
    "prettier": "^3.5.3",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "typescript-eslint": "^8.26.1",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.2.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.44.2",
    "@rollup/rollup-win32-x64-gnu": "4.52.4"
  }
}
